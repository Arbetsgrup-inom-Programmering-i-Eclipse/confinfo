<Window x:Name="window1" x:Class="ConfInfo.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ConfInfo"
        mc:Ignorable="d"
        Title="Window1" Height="150" Width="800">
    <WrapPanel HorizontalAlignment="Left" Height="118" VerticalAlignment="Top" Width="774" RenderTransformOrigin="-0.222,-0.204" Margin="10,0,0,0">

        <DataGrid x:Name="dataGrid" AutoGenerateColumns="True" HorizontalAlignment="Left" VerticalAlignment="Top" Height="88" Width="773" Margin="0,0,-2,0" RenderTransformOrigin="0.242,0.559"/>
        <Button IsDefault="True" x:Name="closeButton" Content="Close" Width="75" Click="closeButton_Click"/>
        <Button x:Name="AddButton" Content="Add" Width="75" Click="addButton_Click"/>

    </WrapPanel>


</Window>
